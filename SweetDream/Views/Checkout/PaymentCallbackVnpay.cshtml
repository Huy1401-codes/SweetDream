@model SweetDream.Models.VnPay.PaymentResponseModel

@{

    ViewData["title"] = "Vnpay payment transaction information results successful";

}

<h5>VnPay payment successful with:</h5>

<h6>OrderId : @Model.OrderId</h6>

<h6>OrderInfo: @Model.OrderDescription</h6>

<h6>Total: @Model.PaymentMethod</h6>

<h6>Transaction ID: @Model.TransactionId</h6>

<h6>Payment code: @Model.PaymentId</h6>

<h5>View <a asp-controller="Profiles" asp-action="Orders">history of placed orders</a></h5>